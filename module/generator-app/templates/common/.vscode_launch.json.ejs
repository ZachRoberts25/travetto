  "version": "0.2.0",
  "configurations": [
  {
    "type": "node",
    "request": "launch",
    "name": "Launch via Node",
    "env": {
      "TRV_APP_NAME": "<%= name %>"
    },
    "program": "${workspaceFolder}/node_modules/@travetto/base/bin/bootstrap",
    "cwd": "${workspaceFolder}",
    "skipFiles": [
      "<node_internals>/**",
      "**/@travetto/context/**/*",
      "**/@travetto/base/**/stacktrace.*",
      "**/@travetto/compiler/**/proxy.*",
      "**/node_modules/cls-hooked/**/*",
      "**/node_modules/trace/**/*",
      "**/node_modules/stack-chain/**/*"
    ]
  }]
}